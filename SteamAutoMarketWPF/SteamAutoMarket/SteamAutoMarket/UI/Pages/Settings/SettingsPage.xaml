<UserControl x:Class="SteamAutoMarket.UI.Pages.Settings.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/UI/Pages/Settings/Appearance.xaml">
            <mui:ModernTab.Links>
                <mui:Link x:Name="AppearanceMenuLink" DisplayName="Appearance" Source="/UI/Pages/Settings/Appearance.xaml" />
                <mui:Link x:Name="LicenseMenuLink" DisplayName="license" Source="/UI/Pages/Settings/License.xaml" />
                <mui:Link x:Name="MarketMenuLink" DisplayName="market" Source="/UI/Pages/Settings/MarketSettings.xaml" />
                <mui:Link x:Name="CacheMenuLink" DisplayName="cache" Source="/UI/Pages/Settings/CacheSettings.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>